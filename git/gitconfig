[alias]
    logx = log --remotes --graph --branches --decorate=short --oneline --abbrev-commit
    outgoing = "!git log --oneline origin/$(git rev-parse --abbrev-ref HEAD).."
    incoming = "!git log --oneline ..origin/$(git rev-parse --abbrev-ref HEAD)"
    co = checkout
    st = status
[core]
	editor = /usr/local/bin/vim
