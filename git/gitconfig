[push]
	default = simple
[alias]
    logx = log --remotes --graph --branches --format='%C(auto)%h%d%Creset %s %Cgreen(%ar) %Cblue<%an>%Creset'
    outgoing = "!git log --oneline origin/$(git rev-parse --abbrev-ref HEAD).."
    incoming = "!git log --oneline ..origin/$(git rev-parse --abbrev-ref HEAD)"
    co = checkout
    st = status
[core]
	editor = mvim -v
    excludesfile = ~/.git_excludes
